package lort;
import java.util.Scanner;


public class sorm {
	public static void main(String[] args) {
		class my{
			
			private int pass= 213213;
	
		}
		my lot = new my();
		lot.pass = 213213;
		Scanner scan = new Scanner(System.in);
		System.out.print("Введите пароль:");
		int check = scan.nextInt();
		if (check == lot.pass) {
			System.out.print("ok");
			
		}else {
			System.out.print("no ok");
		}
		
	}

	
}
