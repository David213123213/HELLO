import java.util.Scanner;

public class Programm {
	public static void main (String[] args) {

		Scanner k = new Scanner(System.in);
		System.out.print("Введите число: ");
		int day = k.nextInt();
		String day1 = ("понедельник");
		String day2 = ("понедельник");
		String day3 = ("понедельник");
		String day4 = ("понедельник");
		String day5 = ("понедельник");
		String day6 = ("понедельник");
		switch (day) {
		case day1 : 
				System.out.print("1");
		break;
		case day2 : 
				System.out.print("2");
		break;
		case day3 :
				System.out.print("среда");
		break;
		case day4 : 
				System.out.print("четверг");
		break;
		case day5 : 
				System.out.print("пятница");
		break;
		case day6 : 
				System.out.print("суббота");
		case day7 : 
			System.out.print("суббота");
		break;
		default : 
				System.out.print("НЕТ");
		
		}
		
		}
	}
